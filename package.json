{
  "name": "aframe-super-keyboard",
  "version": "2.1.0",
  "description": "Lightweight, functional, and fully customizable VR keyboard for A-Frame.",
  "main": "index.js",
  "unpkg": "dist/aframe-super-keyboard.min.js",
  "scripts": {
    "build": "webpack index.js dist/aframe-super-keyboard.js",
    "dev": "budo index.js:dist/aframe-super-keyboard.min.js --port 7000 --live --open",
    "dist": "npm run build && uglifyjs dist/aframe-super-keyboard.js > dist/aframe-super-keyboard.min.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermedium/aframe-super-keyboard.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "super-keyboard"
  ],
  "author": "Diego F. Goberna <diego@feiss.be>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supermedium/aframe-super-keyboard/issues"
  },
  "homepage": "https://github.com/supermedium/aframe-super-keyboard#readme",
  "devDependencies": {
    "aframe": "*",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "karma": "^0.13.15",
    "karma-browserify": "^8.1.0",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^10.1.0",
    "randomcolor": "^0.4.4",
    "semistandard": "^16.0.1",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "webpack": "3.8.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "uglify-js": "^3.3.23",
    "uglifyjs": "^2.4.11"
  }
}
